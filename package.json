{
  "name": "g-canales-io",
  "version": "1.0.0",
  "description": "Fetching gitignore files",
  "scripts": {
    "start:web": "npm run build -w web && npm run serve -w web",
    "start:api": "npm run start -w api",
    "start:web-dev": "npm run dev -w web -- --host",
    "start:api-dev": "npm run dev -w api",
    "start:dev": "concurrently \"npm run start:web-dev\" \"npm run start:api-dev\"",
    "start:prod": "concurrently \"npm run start:web\" \"npm run start:api\""
  },
  "repository": {
    "type": "git",
    "url": "gitignore@g.canales.io:app/"
  },
  "author": "Vicente Canales <vicente@canales.io>",
  "license": "MIT",
  "workspaces": [
    "web",
    "api"
  ],
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "concurrently": "^6.2.2"
  },
  "dependencies": {
    "isomorphic-unfetch": "^3.1.0"
  }
}
